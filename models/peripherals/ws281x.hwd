// RIOT Configuration for ws2812 neopixel led strip

Peripheral WS281X
    type: Actuator
    vcc: 5
    pins:
		- power:
			name: vcc
			number: 1
			type: 5V
		- power:
			name: gnd
			number: 2
			type: GND
		- io_pin:
            functions: gpio
			name: data_in
			number: 3
		- io_pin:
            functions: gpio
			name: data_out
			number: 4
end
